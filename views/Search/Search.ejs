<body id="top" style="padding-top: 60px;">
    <% include ../Partials/_navbar //relative path %>
    <div class="container">
        
        <nav aria-label="Page navigation">
            <ul class="pagination">
            <!-- <li>
                <a href="#" aria-label="Previous">
                <span aria-hidden="true">&laquo;</span>
                </a>
            </li> -->
            <% if(locals.result.length > 0){ %>
